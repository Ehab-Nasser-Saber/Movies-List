: null,\n      currentIndex: -1,\n    };\n  },\n  methods: {\n    onDataChange(items) {\n      let _movies = [];\n\n      items.forEach((item) => {\n        let key = item.key;\n        let data = item.val();\n        _movies.push({\n          key: key,\n          title: data.title,\n          Genre: data.Genre,\n          published: data.published,\n        });\n      });\n\n      this.movies = _movies;\n    },\n\n    refreshList() {\n      this.currentMovie = null;\n      this.currentIndex = -1;\n    },\n\n    setActiveMovie(movie, index) {\n      this.currentMovie = movie;\n      this.currentIndex = index;\n    },\n\n    removeAllMovies() {\n      MovieDataService.deleteAll()\n        .then(() => {\n          this.refreshList();\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n  mounted() {\n    MovieDataService.getAll().on(\"value\", this.onDataChange);\n  },\n  beforeUnmount() {\n    MovieDataService.getAll().off(\"value\", this.onDataChange);\n  },\n};\n</script>\n\n<style>\n.list {\n  text-align: left;\n  max-width: 750px;\n  margin: auto;\n}\n</style>\n"],"sourceRoot":""}]}