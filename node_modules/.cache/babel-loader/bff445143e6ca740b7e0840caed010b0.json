vBR,EAuBe,KAvBf,EAuBsB;AAC3B,WAAK,YAAL,GAAoB,KAApB;AACA,WAAK,YAAL,GAAoB,KAApB;AACD,KA1BM;AA4BP,IAAA,eA5BO,6BA4BW;AAAA;;AAChB,MAAA,gBAAgB,CAAC,SAAjB,GACG,IADH,CACQ,YAAM;AACV,QAAA,KAAI,CAAC,WAAL;AACD,OAHH,EAIG,KAJH,CAIS,UAAC,KAAD,EAAW;AAChB,QAAA,OAAO,CAAC,GAAR,CAAY,KAAZ;AACD,OANH;AAOD;AApCM,GAVI;AAgDb,EAAA,OAhDa,qBAgDH;AACR,IAAA,gBAAgB,CAAC,MAAjB,GAA0B,EAA1B,CAA6B,OAA7B,EAAsC,KAAK,YAA3C;AACD,GAlDY;AAmDb,EAAA,aAnDa,2BAmDG;AACd,IAAA,gBAAgB,CAAC,MAAjB,GAA0B,GAA1B,CAA8B,OAA9B,EAAuC,KAAK,YAA5C;AACD;AArDY,CAAf","sourcesContent":["<template>\n  <div class=\"list row\">\n    <div class=\"col-md-6\">\n      <h4>Movies List</h4>\n      <ul class=\"list-group\">\n        <li\n          class=\"list-group-item\"\n          :class=\"{ active: index == currentIndex }\"\n          v-for=\"(movie, index) in movies\"\n          :key=\"index\"\n          @click=\"setActiveMovie(movie, index)\"\n        >\n          {{ movie.title }}\n        </li>\n      </ul>\n\n      <button class=\"m-3 btn btn-sm btn-danger\" @click=\"removeAllMovies\">\n        Remove All\n      </button>\n    </div>\n    <div class=\"col-md-6\">\n      <div v-if=\"currentMovie\">\n        <movie-details :movie=\"currentMovie\" @refreshList=\"refreshList\" />\n      </div>\n      <div v-else>\n        <br />\n        <p>Please click on a Movie...</p>\n      </div>\n    </div>\n  </div>\n</template>\n\n<script>\nimport MovieDataService from \"../services/MovieDataService\";\nimport MovieDetails from \"./Movie\";\n\nexport default {\n  name: \"movies-list\",\n  components: { MovieDetails },\n  data() {\n    return {\n      movies: [],\n      currentMovie: null,\n      currentIndex: -1,\n    };\n  },\n  methods: {\n    onDataChange(items) {\n      let _movies = [];\n\n      items.forEach((item) => {\n        let key = item.key;\n        let data = item.val();\n        _movies.push({\n          key: key,\n          title: data.title,\n          Genre: data.Genre,\n          published: data.published,\n        });\n      });\n\n      this.movies = _movies;\n    },\n\n    refreshList() {\n      this.currentMovie = null;\n      this.currentIndex = -1;\n    },\n\n    setActiveMovie(movie, index) {\n      this.currentMovie = movie;\n      this.currentIndex = index;\n    },\n\n    removeAllMovies() {\n      MovieDataService.deleteAll()\n        .then(() => {\n          this.refreshList();\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n  mounted() {\n    MovieDataService.getAll().on(\"value\", this.onDataChange);\n  },\n  beforeUnmount() {\n    MovieDataService.getAll().off(\"value\", this.onDataChange);\n  },\n};\n</script>\n\n<style>\n.list {\n  text-align: left;\n  max-width: 750px;\n  margin: auto;\n}\n</style>\n"],"sourceRoot":""}]}