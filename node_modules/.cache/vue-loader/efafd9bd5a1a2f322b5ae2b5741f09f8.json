ch((error) => {\n          console.log(error);\n        });\n    },\n\n    deleteMovie() {\n      MovieDataService.delete(this.currentMovie.key)\n        .then(() => {\n          this.$emit(\"refreshList\");\n        })\n        .catch((error) => {\n          console.log(error);\n        });\n    },\n  },\n  mounted() {\n    this.message = \"\";\n    this.currentMovie = { ...this.movie };\n  },\n};\n</script>\n\n<style>\n.edit-form {\n  max-width: 300px;\n  margin: auto;\n}\n</style>\n"]}]}