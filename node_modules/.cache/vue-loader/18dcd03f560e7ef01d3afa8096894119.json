rol\"\n          id=\"title\"\n          required\n          v-model=\"movie.title\"\n          name=\"title\"\n        />\n      </div>\n\n      <div class=\"form-group\">\n        <label for=\"description\">Description</label>\n        <input\n          class=\"form-control\"\n          id=\"description\"\n          required\n          v-model=\"movie.description\"\n          name=\"description\"\n        />\n      </div>\n\n      <button @click=\"saveMovie\" class=\"btn btn-success\">Submit</button>\n    </div>\n\n    <div v-else>\n      <h4>You submitted successfully!</h4>\n      <button class=\"btn btn-success\" @click=\"newMovie\">Add</button>\n    </div>\n  </div>\n</template>\n\n<script>\nimport MovieDataService from \"../services/MovieDataService\";\n\nexport default {\n  name: \"add-movie\",\n  data() {\n    return {\n      movie: {\n        title: \"\",\n        description: \"\",\n        published: false\n      },\n      submitted: false\n    };\n  },\n  methods: {\n    saveMovie() {\n      const data = {\n        title: this.movie.title,\n        description: this.movie.description,\n        published: false\n      };\n\n      MovieDataService.create(data)\n        .then(() => {\n          console.log(\"Created new item successfully!\");\n          this.submitted = true;\n        })\n        .catch(error => {\n          console.log(error);\n        });\n    },\n    \n    newMovie() {\n      this.submitted = false;\n      this.movie = {\n        title: \"\",\n        description: \"\",\n        published: false\n      };\n    }\n  }\n};\n</script>\n\n<style>\n.submit-form {\n  max-width: 300px;\n  margin: auto;\n}\n</style>\n"]}]}